        {
          "label": "B",
          "text": "Longitudinal fracture- Right temporal bone",
          "correct": true
        },
        {
          "label": "C",
          "text": "Transverse fracture- Left temporal bone",
          "correct": false
        },
        {
          "label": "D",
          "text": "Transverse fracture- Right temporal bone",
          "correct": false
        }
      ],
      "correct_answer": "B. Longitudinal fracture- Right temporal bone",
      "question_images": [
