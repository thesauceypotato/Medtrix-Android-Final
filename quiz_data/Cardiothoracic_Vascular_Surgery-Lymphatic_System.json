      "options": [
        {
          "label": "A",
          "text": "Lymphangiosarcoma",
          "correct": true
        },
        {
          "label": "B",
          "text": "Venous thrombosis",
          "correct": false
        },
        {
          "label": "C",
          "text": "Cellulitis",
          "correct": false
        },
        {
          "label": "D",
          "text": "Lymphedema",
          "correct": false
        }
      ],
      "correct_answer": "A. Lymphangiosarcoma",
      "question_images": [
        "https://image.prepladder.com/content/YZArRuhKexnfoCWTKpyo1739957732.png"
      ],
