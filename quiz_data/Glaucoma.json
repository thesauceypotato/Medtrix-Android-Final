    {
      "text": "Which of the following is not a recognized risk factor for Primary Open-Angle Glaucoma?",
      "options": [
        {
          "label": "A",
          "text": "Elevated intraocular pressure",
          "correct": false
        },
        {
          "label": "B",
          "text": "Diastolic perfusion pressure of more than 55 mm Hg",
          "correct": true
        },
        {
          "label": "C",
          "text": "Family history of glaucoma",
          "correct": false
        },
        {
          "label": "D",
          "text": "High myopia",
          "correct": false
        }
      ],
      "correct_answer": "B. Diastolic perfusion pressure of more than 55 mm Hg",
