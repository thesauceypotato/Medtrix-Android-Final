        {
          "label": "A",
          "text": "Topical beta-blockers and oral carbonic anhydrase inhibitors",
          "correct": false
        },
        {
          "label": "B",
          "text": "Panretinal photocoagulation",
          "correct": true
        },
        {
          "label": "C",
          "text": "Systemic corticosteroids",
          "correct": false
        },
        {
          "label": "D",
          "text": "VEGF inhibitors",
          "correct": false
        }
      ],
      "correct_answer": "B. Panretinal photocoagulation",
