      "explanation": "<p style='font-size: 10px; color: #808080; font-style: italic;'></p>",
      "bot": "@dams_new_robot",
      "audio": "https://image.prepladder.com/content/3mYFh3G1qwx21FWgBrLw1748246573.mp3",
