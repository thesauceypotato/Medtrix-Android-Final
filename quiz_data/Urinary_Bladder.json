        },
        {
          "label": "B",
          "text": "Emergency laparotomy",
          "correct": true
        },
        {
          "label": "C",
          "text": "IV antibiotics and close monitoring",
          "correct": false
        },
        {
          "label": "D",
          "text": "Delayed surgical repair",
          "correct": false
        }
      ],
      "correct_answer": "B. Emergency laparotomy",
      "question_images": [],
      "explanation_images": [
        "https://image.prepladder.com/content/jEF8M7RMfPObugFctsNa1739966976.png"
