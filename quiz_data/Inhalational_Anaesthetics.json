      "options": [
        {
          "label": "A",
          "text": "Decreased concentration of Isoflurane in the alveoli",
          "correct": false
        },
        {
          "label": "B",
          "text": "Increased concentration of Isoflurane in the alveoli",
          "correct": true
        },
        {
          "label": "C",
          "text": "Increase concentration of Nitrous oxide in alveoli",
          "correct": false
        },
        {
          "label": "D",
          "text": "No effect on the concentration of gases in the alveoli",
          "correct": false
        }
      ],
      "correct_answer": "B. Increased concentration of Isoflurane in the alveoli",
