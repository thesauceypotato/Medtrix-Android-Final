          "label": "B",
          "text": "a-2, b-3, c-1, d-4",
          "correct": false
        },
        {
          "label": "C",
          "text": "a-1, b-2, c-3, d-4",
          "correct": false
        },
        {
          "label": "D",
          "text": "a-4, b-3, c-4, d-2",
          "correct": false
        }
