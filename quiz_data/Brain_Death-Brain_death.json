  "questions": [
    {
      "text": "Which of the following is true about brain death?",
      "options": [
        {
          "label": "A",
          "text": "Complete loss of cerebral cortical function but preservation of brainstem reflexes.",
          "correct": false
        },
        {
          "label": "B",
          "text": "Irreversible loss of all brain function, including the brainstem, with inability to breathe independently.",
          "correct": true
        },
        {
          "label": "C",
          "text": "Persistent vegetative state where higher brain functions are lost, but spontaneous respiratory effort is present.",
          "correct": false
        },
        {
          "label": "D",
          "text": "Reversible coma with absence of brainstem reflexes.",
          "correct": false
        }
      ],
      "correct_answer": "B. Irreversible loss of all brain function, including the brainstem, with inability to breathe independently.",
      "question_images": [],
      "explanation_images": [],
      "explanation": "<p style='font-size: 10px; color: #808080; font-style: italic;'></p>",
      "bot": "@dams_new_robot",
