      "correct_answer": "D. 1- Pirogoff, 2- Chopart, 3- Lisfranc",
      "question_images": [
        "https://image.prepladder.com/content/zs5fvy4ZviEhsJqnQC1F1738315800.png"
      ],
      "explanation_images": [
        "https://image.prepladder.com/content/tO6INj7lhSQoiZCzJB1Q1738315919.png",
        "https://image.prepladder.com/content/tzwiE3h37A5emI4J0g461738315961.png",
        "https://image.prepladder.com/content/bo5mQ5F6kk5hdjDYE4wl1738315983.png",
