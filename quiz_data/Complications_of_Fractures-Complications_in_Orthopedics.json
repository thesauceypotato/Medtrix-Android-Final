        },
        {
          "label": "C",
          "text": "Nerve neuroma",
          "correct": false
        },
        {
          "label": "D",
          "text": "Skin ulceration",
          "correct": false
        }
      ],
      "correct_answer": "B. Phantom limb sensation and phantom pain",
