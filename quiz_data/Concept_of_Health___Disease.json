        {
          "label": "B",
          "text": "Measles",
          "correct": true
        },
        {
          "label": "C",
          "text": "HIV",
          "correct": false
        },
        {
          "label": "D",
          "text": "Cancer",
          "correct": false
        }
      ],
      "correct_answer": "B. Measles",
      "question_images": [],
      "explanation_images": [
