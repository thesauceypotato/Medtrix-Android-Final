        {
          "label": "C",
          "text": "Takayama Test",
          "correct": true
        },
        {
          "label": "D",
          "text": "Precipitin Test",
          "correct": false
        }
      ],
      "correct_answer": "C. Takayama Test",
      "question_images": [],
      "explanation_images": [
