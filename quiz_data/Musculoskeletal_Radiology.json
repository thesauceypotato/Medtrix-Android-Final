        {
          "label": "A",
          "text": "Hitchhiker's thumb deformity",
          "correct": false
        },
        {
          "label": "B",
          "text": "Boutonniere deformity",
          "correct": false
        },
        {
          "label": "C",
          "text": "Swan neck deformity",
          "correct": false
        },
        {
          "label": "D",
          "text": "Calcinosis cutis",
          "correct": true
        }
