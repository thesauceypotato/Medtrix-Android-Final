        {
          "label": "B",
          "text": "Telogen effluvium",
          "correct": false
        },
        {
          "label": "C",
          "text": "Androgenic alopecia",
          "correct": true
        },
        {
          "label": "D",
          "text": "Lupus erythematosus",
          "correct": false
        }
