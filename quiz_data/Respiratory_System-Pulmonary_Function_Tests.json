          "correct": false
        },
        {
          "label": "B",
          "text": "TV + IRV + ERV",
          "correct": true
        },
        {
          "label": "C",
          "text": "TV + IRV",
          "correct": false
        },
        {
          "label": "D",
          "text": "ERV + RV",
          "correct": false
        }
      ],
      "correct_answer": "B. TV + IRV + ERV",
      "question_images": [],
