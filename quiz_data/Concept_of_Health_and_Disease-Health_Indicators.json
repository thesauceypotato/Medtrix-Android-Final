      "options": [
        {
          "label": "A",
          "text": "Notification rate",
          "correct": false
        },
        {
          "label": "B",
          "text": "Incidence",
          "correct": false
        },
        {
          "label": "C",
          "text": "Attendance rate",
          "correct": false
        },
        {
          "label": "D",
          "text": "Expectation of life",
          "correct": true
        }
      ],
      "correct_answer": "D. Expectation of life",
      "question_images": [],
