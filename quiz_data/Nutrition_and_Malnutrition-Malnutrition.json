    },
    {
      "text": "A 2 year old malnourished child is assessed using mid-upper arm circumference (MUAC), and the measurement is 10 cm. What does this indicate?",
      "options": [
        {
          "label": "A",
          "text": "Moderate Stunting",
          "correct": false
        },
        {
          "label": "B",
          "text": "Severe Wasting",
          "correct": true
        },
        {
          "label": "C",
          "text": "Chronic Malnutrition",
          "correct": false
        },
        {
          "label": "D",
          "text": "Obesity",
          "correct": false
        }
      ],
      "correct_answer": "B. Severe Wasting",
      "question_images": [],
      "explanation_images": [],
      "explanation": "<p style='font-size: 10px; color: #808080; font-style: italic;'></p>",
      "bot": "@dams_new_robot",
      "audio": "https://image.prepladder.com/content/cPiJnqBVNWOXhHpEKZla1743079981.mp3",
      "video": "",
