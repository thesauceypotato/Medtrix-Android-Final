        },
        {
          "label": "C",
          "text": "Lachman",
          "correct": true
        },
        {
          "label": "D",
          "text": "Anterior drawer for ACL",
          "correct": false
        }
      ],
      "correct_answer": "C. Lachman",
      "question_images": [
