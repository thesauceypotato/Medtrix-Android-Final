        },
        {
          "label": "B",
          "text": "Aconite",
          "correct": false
        },
        {
          "label": "C",
          "text": "Arsenic",
          "correct": false
        },
        {
          "label": "D",
          "text": "Alcohol",
          "correct": true
        }
      ],
      "correct_answer": "D. Alcohol",
