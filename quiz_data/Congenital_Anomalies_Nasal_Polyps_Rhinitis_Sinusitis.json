        {
          "label": "B",
          "text": "Non-allergic rhinitis",
          "correct": false
        },
        {
          "label": "C",
          "text": "Perennial allergic rhinitis",
          "correct": true
        },
        {
          "label": "D",
          "text": "Allergic asthma",
          "correct": false
        }
      ],
      "correct_answer": "C. Perennial allergic rhinitis",
