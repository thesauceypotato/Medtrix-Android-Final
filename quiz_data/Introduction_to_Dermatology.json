        {
          "label": "A",
          "text": "Stratum Spinosum",
          "correct": false
        },
        {
          "label": "B",
          "text": "Stratum Granulosum",
          "correct": false
        },
        {
          "label": "C",
          "text": "Stratum Basale",
          "correct": false
        },
        {
          "label": "D",
          "text": "Stratum Corneum",
          "correct": true
        }
      ],
      "correct_answer": "D. Stratum Corneum",
