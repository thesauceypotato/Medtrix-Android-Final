        {
          "label": "A",
          "text": "Munro\u2019s micro abscess",
          "correct": false
        },
        {
          "label": "B",
          "text": "Pautrier's microabscess",
          "correct": false
        },
        {
          "label": "C",
          "text": "Elongation of rete ridges",
          "correct": true
        },
        {
          "label": "D",
          "text": "Flattening of rete ridges",
          "correct": false
        }
      ],
      "correct_answer": "C. Elongation of rete ridges",
