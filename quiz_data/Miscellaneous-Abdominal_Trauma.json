        {
          "label": "A",
          "text": "1, 2, 3, 6, 7",
          "correct": false
        },
        {
          "label": "B",
          "text": "3, 4, 5, 6",
          "correct": false
        },
        {
          "label": "C",
          "text": "2, 4, 5, 7",
          "correct": true
        },
        {
          "label": "D",
          "text": "1, 2, 5, 7",
          "correct": false
        }
      ],
      "correct_answer": "C. 2, 4, 5, 7",
